<section class="settings-panel" id="settingsPanel">
  <h2>Settings</h2>
  <p class="close-panel" onclick="Lib.default.closeSettings('settingsPanel')">X</p>
  <%- include('./components/params.ejs',{title:"Headers",baseID:"defHd" }) %>
